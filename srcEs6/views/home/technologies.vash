@html.extend('shared/layout', function(model) {
  @html.block('body', function (model) {  
      <h2>Technologies Used:</h2>
        <ul>
            @model.technologies.forEach(function(item){
                <li>@item</li>
            })           
        </ul>
        <br/>
        <h2>Patterns and Practices Followed:</h2>
        <ul>
            @model.patternsAndPractices.forEach(function(item){
                <li>@item</li>
            })           
        </ul>
  })
})


